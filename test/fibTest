int scanf(string fmt, ...);
int printf(string fmt, ...);

void out(int x) {
    printf("%ld\n", x);
}
void outf(float x) {
    printf("%lf\n", x);
}

int fib(int x) {
    if(x < 2) {
        return 1;
    }
    return fib(x-1) + fib(x-2);
}

int main() {
    int a;
    scanf("%d", &a);
    printf("%d\n", fib(a));
    return 0;
}